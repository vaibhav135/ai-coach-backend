{
  "name": "ai-coach-backend",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "dev:agent": "tsx watch src/agent/worker.ts",
    "build": "tsc && tsc-alias",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio",
    "lint": "biome check .",
    "lint:fix": "biome check --write .",
    "format": "biome format --write .",
    "prepare": "husky"
  },
  "lint-staged": {
    "*.{js,ts,jsx,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "@livekit/agents": "^1.0.42",
    "@livekit/agents-plugin-openai": "^1.0.42",
    "dotenv": "^17.2.4",
    "drizzle-orm": "^0.45.1",
    "google-auth-library": "^10.5.0",
    "hono": "^4.11.8",
    "jose": "^6.1.3",
    "livekit-server-sdk": "^2.15.0",
    "pg": "^8.18.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.14",
    "@types/node": "^25.2.1",
    "@types/pg": "^8.16.0",
    "drizzle-kit": "^0.31.8",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
